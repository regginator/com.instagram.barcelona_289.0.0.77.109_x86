<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/cds_white_a20" />
            <stroke android:width="@dimen/account_recs_header_image_margin" android:color="@color/fds_white_alpha40" />
            <corners android:radius="@dimen/abc_action_bar_elevation_material" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/fds_transparent" />
            <stroke android:width="@dimen/account_recs_header_image_margin" android:color="@color/fds_white_alpha40" />
            <corners android:radius="@dimen/abc_action_bar_elevation_material" />
        </shape>
    </item>
</selector>