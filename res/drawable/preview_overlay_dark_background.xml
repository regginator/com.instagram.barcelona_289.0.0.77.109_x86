<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="oval">
                    <stroke android:width="1.0px" android:color="#66a5a9ac" />
                </shape>
            </item>
            <item>
                <shape android:shape="oval">
                    <stroke android:width="1.0px" android:color="#33ffffff" />
                </shape>
            </item>
        </selector>
    </item>
    <item>
        <inset android:insetLeft="1.0px" android:insetRight="1.0px" android:insetTop="1.0px" android:insetBottom="1.0px">
            <selector>
                <item android:state_pressed="true">
                    <shape android:shape="oval">
                        <solid android:color="@color/grey_9" />
                    </shape>
                </item>
                <item android:state_activated="true">
                    <shape android:shape="oval">
                        <solid android:color="#bf7ab2e6" />
                    </shape>
                </item>
                <item>
                    <shape android:shape="oval">
                        <solid android:color="@color/grey_7_75_transparent" />
                    </shape>
                </item>
            </selector>
        </inset>
    </item>
</layer-list>