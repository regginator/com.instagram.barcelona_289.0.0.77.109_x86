<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/secondary_text_default_material_dark" />
            <corners android:radius="@dimen/abc_dialog_padding_material" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/design_dark_default_color_on_background" />
            <corners android:radius="@dimen/abc_dialog_padding_material" />
        </shape>
    </item>
</selector>